{
  "name": "claimr-documentation",
  "version": "0.0.0",
  "private": false,
  "license": "UNLICENCED",
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "serve": "npx serve build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-beta.3",
    "@docusaurus/preset-classic": "^2.0.0-beta.0",
    "clsx": "^1.1.1",
    "docusaurus-plugin-plausible": "^0.0.5",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-beta.0",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/react": "17.0.11",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "typescript": "^4.3.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 110
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,yml,yaml,md,json}": "prettier --write"
  }
}
